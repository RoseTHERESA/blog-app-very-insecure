Search Results:

<table>
  <tbody>
  <% @search_results.each do |res| %>
    <tr>
      <td><h4><%= res.title %></h4><br />
      <%= res.content %>
      <h6>Written by <%= res.user.email %></h6>
      </td>
      <td><%= link_to 'Show', res %><br />
      <%= link_to 'Edit', edit_post_path(res.id) %><br />
      <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>